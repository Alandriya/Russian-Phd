@article{gorshenin2020efficiency,
	author = {Gorshenin, A. K. and Shcherbinina, A. A.},
	title = {Efficiency of the method for detecting normal mixture signals with pre-estimated Gaussian mixture noise},
	journal = {Pattern Recognition and Image Analysis: Advances in Mathematical Theory and Applications},
	year = {2020},
	volume = {30},
	number = {3},
	issn = {1555-6212; 1054-6618},
	doi = {10.1134/S1054661820030074},
	pages = {470--479},
	publisher = {Pleiades Publishing, Ltd},
	address = {Road Town, United Kingdom},
	annote = {The paper discusses the effectiveness of the method for determining the parameters of the useful signal in the sliding window mode, provided that it is possible to obtain preliminary estimates for the noise distribution. For the statistical experiment, 24 samples had been generated with different ratios between the signal and noise parameters. Implementation of the computational procedures for the adaptive method in the Python programming language is proposed. For the test samples, it is demonstrated that the magnitude of the error in evaluating the parameters in the vast majority of cases does not exceed value 1 (in terms of the standard RMSE metrics). In addition, an effective two-pass method for detecting the moment of the appearance of a meaningful signal in the noisy data is proposed. The results of its operation are also demonstrated on the example of the mentioned test samples.},
	language = {english}
}
